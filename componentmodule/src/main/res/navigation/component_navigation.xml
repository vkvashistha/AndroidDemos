            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools" android:id="@+id/component_navigation"
            app:startDestination="@id/componentFragment">

    <fragment android:id="@+id/componentFragment" android:name="com.example.componentmodule.ComponentFragment"
              android:label="fragment_component" tools:layout="@layout/fragment_component">
