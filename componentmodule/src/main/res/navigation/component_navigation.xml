                app:destination="@id/componentChildFragment"/>
    </fragment>
    <fragment android:id="@+id/componentChildFragment" android:name="com.example.componentmodule.ComponentChildFragment"
              android:label="fragment_component_child" tools:layout="@layout/fragment_component_child"/>
</navigation>